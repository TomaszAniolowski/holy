    <search:element ns="http://holy.arch.com/data/pericope" name="id" xmlns:es="http://marklogic.com/entity-services"/>
  </search:range>
</search:constraint> 
-->
  <search:constraint name="Supplement">
    <search:container>
      <search:json-property>Supplement</search:json-property>
    </search:container>
  </search:constraint>
  <search:constraint name="Supplement-xml">
    <search:container>
      <search:element ns="http://holy.arch.com/extension/supplement" name="Supplement"/>
    </search:container>
  </search:constraint>
  <!--The name of this constraint is a duplicate in the generated XML. It is within a comment so that the XML may be valid, but you may need to edit for your use case.
 <search:constraint name="id" xmlns:search="http://marklogic.com/appservices/search">
  <search:value>
    <search:element ns="" name="id"/>
  </search:value>
</search:constraint> 
-->
  <!--The name of this constraint is a duplicate in the generated XML. It is within a comment so that the XML may be valid, but you may need to edit for your use case.
 <search:constraint name="id" xmlns:search="http://marklogic.com/appservices/search">
  <search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/">
    <search:element ns="http://holy.arch.com/extension/supplement" name="id" xmlns:es="http://marklogic.com/entity-services"/>
  </search:range>
</search:constraint> 
-->
  <search:constraint name="Chapter">
    <search:container>
      <search:json-property>Chapter</search:json-property>
    </search:container>
  </search:constraint>
  <search:constraint name="Chapter-xml">
    <search:container>
      <search:element ns="http://holy.arch.com/data/chapter" name="Chapter"/>
    </search:container>
  </search:constraint>
  <!--The name of this constraint is a duplicate in the generated XML. It is within a comment so that the XML may be valid, but you may need to edit for your use case.
 <search:constraint name="id" xmlns:search="http://marklogic.com/appservices/search">
  <search:value>
    <search:element ns="" name="id"/>
  </search:value>
</search:constraint> 
-->
  <search:constraint name="Chapter.id">
    <search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/">
      <search:path-index xmlns:es="http://marklogic.com/entity-services" xmlns:ch="http://holy.arch.com/data/chapter">//*:instance/ch:Chapter/ch:id</search:path-index>
      <search:facet-option>limit=25</search:facet-option>
      <search:facet-option>frequency-order</search:facet-option>
      <search:facet-option>descending</search:facet-option>
    </search:range>
  </search:constraint>
  <search:constraint name="Tome">
    <search:container>
      <search:json-property>Tome</search:json-property>
    </search:container>
  </search:constraint>
  <search:constraint name="Tome-xml">
    <search:container>
      <search:element ns="http://holy.arch.com/data/tome" name="Tome"/>
    </search:container>
  </search:constraint>
  <search:constraint name="siglum">
    <search:value>
      <search:element ns="" name="siglum"/>
    </search:value>
  </search:constraint>
  <!--The name of this constraint is a duplicate in the generated XML. It is within a comment so that the XML may be valid, but you may need to edit for your use case.
 <search:constraint name="siglum" xmlns:search="http://marklogic.com/appservices/search">
  <search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/">
    <search:element ns="http://holy.arch.com/data/tome" name="siglum" xmlns:es="http://marklogic.com/entity-services"/>
  </search:range>
</search:constraint> 
-->
  <search:values name="Verse">
    <search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/">
      <search:element ns="http://holy.arch.com/data/verse" name="id" xmlns:es="http://marklogic.com/entity-services"/>
    </search:range>
  </search:values>
  <search:values name="Pericope">
    <search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/">
      <search:element ns="http://holy.arch.com/data/pericope" name="id" xmlns:es="http://marklogic.com/entity-services"/>
    </search:range>
  </search:values>
  <search:values name="Supplement">
    <search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/">
      <search:element ns="http://holy.arch.com/extension/supplement" name="id" xmlns:es="http://marklogic.com/entity-services"/>
    </search:range>
  </search:values>
  <search:values name="Chapter">
    <search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/">
      <search:path-index xmlns:es="http://marklogic.com/entity-services" xmlns:ch="http://holy.arch.com/data/chapter">//*:instance/ch:Chapter/ch:id</search:path-index>
    </search:range>
  </search:values>
  <search:values name="Tome">
    <search:range type="xs:string" facet="true" collation="http://marklogic.com/collation/">
      <search:element ns="http://holy.arch.com/data/tome" name="siglum" xmlns:es="http://marklogic.com/entity-services"/>
    </search:range>
  </search:values>
  <!--Uncomment to return no results for a blank search, rather than the default of all results
 <search:term xmlns:search="http://marklogic.com/appservices/search">
  <search:empty apply="no-results"/>
</search:term> 
-->
  <search:values name="uris">
    <search:uri/>
  </search:values>
  <!--Change to 'filtered' to exclude false-positives in certain searches-->
  <search:search-option>unfiltered</search:search-option>
  <!--Modify document extraction to change results returned-->
  <search:extract-document-data selected="include">
    <search:extract-path xmlns:es="http://marklogic.com/entity-services" xmlns:v="http://holy.arch.com/data/verse" xmlns:p="http://holy.arch.com/data/pericope" xmlns:supp="http://holy.arch.com/extension/supplement" xmlns:ch="http://holy.arch.com/data/chapter" xmlns:t="http://holy.arch.com/data/tome">//*:instance/(v:Verse|p:Pericope|supp:Supplement|ch:Chapter|t:Tome)</search:extract-path>
    <search:extract-path xmlns:es="http://marklogic.com/entity-services">/*:envelope/*:headers</search:extract-path>
  </search:extract-document-data>
  <!--Change or remove this additional-query to broaden search beyond entity instance documents-->
  <!--To return facets, change this option to 'true' and edit constraints-->
  <search:return-facets>true</search:return-facets>
  <!--To return snippets, comment out or remove this option-->
  <!--<search:transform-results apply="empty-snippet"></search:transform-results>-->
</search:options>
