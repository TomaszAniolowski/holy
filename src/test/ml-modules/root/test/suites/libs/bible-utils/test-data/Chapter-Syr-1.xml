                    <v:sub-number>I</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>Jeżeli pragniesz mądrości, zachowuj przykazania,</v:content>
                    <v:dictionary datatype="array">
                        <supp:Supplement xmlns:supp="http://holy.arch.com/holy-entities/supplement">
                            aab8dfc8543af5cb609537b5db02d934
                        </supp:Supplement>
                    </v:dictionary>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>7d6c03daf1aaec891c702478ea63eb8a</v:id>
                    <v:card-number>26</v:card-number>
                    <v:number>26</v:number>
                    <v:sub-number>II</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>a Pan ci jej użyczy.</v:content>
                    <v:dictionary datatype="array">
                        <supp:Supplement xmlns:supp="http://holy.arch.com/holy-entities/supplement">
                            05aece6b6fd41c7414a49e07d0072e35
                        </supp:Supplement>
                    </v:dictionary>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
