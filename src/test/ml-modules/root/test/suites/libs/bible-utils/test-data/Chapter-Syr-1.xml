                    <v:card-number>24</v:card-number>
                    <v:number>24</v:number>
                    <v:sub-number>II</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>Wargi wielu będą rozgłaszać jego roztropność.</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
