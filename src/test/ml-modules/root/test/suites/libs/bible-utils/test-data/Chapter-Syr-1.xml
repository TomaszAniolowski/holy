                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>Do czasu powściąga on swój język.</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
