                    <v:card-number>27</v:card-number>
                    <v:number>27</v:number>
                    <v:sub-number>I</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>Mądrość i wychowanie to bojaźń Pana,</v:content>
                    <v:dictionary datatype="array">
                        <supp:Supplement xmlns:supp="http://holy.arch.com/holy-entities/supplement">
                            05aece6b6fd41c7414a49e07d0072e35
                        </supp:Supplement>
                    </v:dictionary>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>3150ff718eea0288c12ef2e15c5b7190</v:id>
                    <v:card-number>27</v:card-number>
                    <v:number>27</v:number>
                    <v:sub-number>II</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>On ma upodobanie w wierności i łagodności.</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>49f1f48bfad1ed233b68c305e12fb543</v:id>
                    <v:card-number>28</v:card-number>
                    <v:number>28</v:number>
                    <v:sub-number>I</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>Nie lekceważ bojaźni Pana,</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>70f017dc2545790c54b3a200e33f1377</v:id>
                    <v:card-number>28</v:card-number>
                    <v:number>28</v:number>
                    <v:sub-number>II</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>nie podchodź do Niego nieszczerze.</v:content>
                    <v:definitions datatype="array">
                        <supp:Supplement xmlns:supp="http://holy.arch.com/holy-entities/supplement">
                            cdd5ee599d1f7304b1b0e792ad8c9cc2
                        </supp:Supplement>
                    </v:definitions>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>c1f78d44cdf52a96e2c44dea9dadad60</v:id>
                    <v:card-number>29</v:card-number>
                    <v:number>29</v:number>
                    <v:sub-number>I</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>Niech ludzie nie uważają cię za obłudnika,</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>038b87941013e118f66562aa4bf25a2c</v:id>
                    <v:card-number>29</v:card-number>
                    <v:number>29</v:number>
                    <v:sub-number>II</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>czuwaj nad swymi wargami.</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>0483b72ac9ce572b940de0d18dc34613</v:id>
                    <v:card-number>30</v:card-number>
                    <v:number>30</v:number>
                    <v:sub-number>I</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>Nie wywyższaj się, abyś nie upadł</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>6465935215fbd469396597e0beef4d09</v:id>
                    <v:card-number>30</v:card-number>
                    <v:number>30</v:number>
                    <v:sub-number>II</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>i nie sprowadził na siebie hańby.</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>ffb3be94f9bdfc1c23f7fa1679727f58</v:id>
                    <v:card-number>30</v:card-number>
                    <v:number>30</v:number>
                    <v:sub-number>III</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>Pan odkryje twoje tajemnice</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>e310ff184982d9fa3d3197e6722ee659</v:id>
                    <v:card-number>30</v:card-number>
                    <v:number>30</v:number>
                    <v:sub-number>IV</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
