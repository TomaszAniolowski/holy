                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
                    <v:content>W skarbcach mądrości – rozumne przysłowia,</v:content>
                </v:Verse>
            </ch:verses>
            <ch:verses datatype="array">
                <v:Verse xmlns:v="http://holy.arch.com/holy-entities/verse">
                    <v:id>e26d39c464c9591ea47b7d9beb268161</v:id>
                    <v:card-number>25</v:card-number>
                    <v:number>25</v:number>
                    <v:sub-number>II</v:sub-number>
                    <v:chapter>
                        <ch:Chapter>99b4be01b5bb3eeb7b9c8ea76d24a1a6</ch:Chapter>
                    </v:chapter>
