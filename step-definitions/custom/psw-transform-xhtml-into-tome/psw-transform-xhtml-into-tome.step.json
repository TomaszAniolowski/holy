{
  "lang": "pl",
  "name": "psw-transform-xhtml-into-tome",
  "description": "This is the custom step to transform xhtml data from pismoswiete.pl to prototype of Tome entity",
  "type": "CUSTOM",
  "version": 1,
  "modulePath": "/custom-modules/custom/psw-transform-xhtml-into-tome/main.sjs",
  "customHook": {},
  "retryLimit": 0,
  "batchSize": 4,
  "threadCount": 16,
  "options": {
    "sourceQuery": "cts.andQuery([cts.collectionQuery(['holy-basic-data','basic-chapter','latest']), cts.elementWordQuery(fn.QName(\"http://holy.arch.com/data/chapter/basic\", \"number\"), \"1\")])",
    "sourceDatabase": "holy-STAGING",
    "targetDatabase": "holy-STAGING",
    "outputFormat": "xml",
    "collections": [
      "holy-basic-data",
      "basic-tome",
      "latest"
    ],
    "additionalCollections": [],
    "permissions": "data-hub-operator,read,data-hub-operator,update",
    "headers": {}
  }
}