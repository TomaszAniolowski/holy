{
  "lang": "pl",
  "name": "psw-transform-xhtml-into-supplement",
  "description": "This is the custom step to transform xhtml data from pismoswiete.pl to prototype of Supplement entity",
  "type": "CUSTOM",
  "version": 1,
  "modulePath": "/custom-modules/custom/psw-transform-xhtml-into-supplement/main.sjs",
  "customHook": {},
  "retryLimit": 0,
  "batchSize": 1,
  "threadCount": 2,
  "options": {
    "sourceQuery": "cts.andQuery([cts.directoryQuery('/BasicChapter/Ag/'), cts.collectionQuery('latest')])",
    "sourceDatabase": "holy-STAGING",
    "targetDatabase": "holy-STAGING",
    "outputFormat": "xml",
    "collections": [
      "holy-basic-data",
      "basic-supplement",
      "latest"
    ],
    "additionalCollections": [],
    "permissions": "data-hub-operator,read,data-hub-operator,update",
    "headers": {}
  }
}